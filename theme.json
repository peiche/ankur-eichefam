        "typography": {
            "fontFamilies": [
                {
                    "fontFamily": "\"Open Sans\", sans-serif",
                    "name": "Open Sans",
                    "slug": "body",
                    "fontFace": [
                        {
                            "fontFamily": "Open Sans",
                            "fontWeight": "400 700",
                            "fontStyle": "normal",
                            "src": [
                                "file:./assets/fonts/open-sans/opensans-variable.ttf"
                            ]
                        }
                    ]
                },
                {
                    "fontFamily": "\"Montserrat\", sans-serif",
                    "name": "Montserrat",
                    "slug": "heading",
                    "fontFace": [
                        {
                            "fontFamily": "Montserrat",
                            "fontWeight": "700",
                            "fontStyle": "normal",
                            "src": [
                                "file:./assets/fonts/montserrat/montserrat-variable.ttf"
                            ]
                        }
                    ]
                },
                {
                    "fontFamily": "\"Source Code Pro\", monospace",
                    "name": "Source Code Pro",
                    "slug": "monospace",
                    "fontFace": [
                        {
                            "fontFamily": "Source Code Pro",
                            "fontWeight": "400 700",
                            "fontStyle": "normal",
                            "src": [
                                "file:./assets/fonts/source-code-pro/sourcecodepro-variable.ttf"
                            ]
